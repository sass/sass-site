{% codeExample 'map-get' %}
  @use "sass:map";
  $font-weights: ("regular": 400, "medium": 500, "bold": 700);

  @debug map.get($font-weights, "medium"); // 500
  @debug map.get($font-weights, "extra-bold"); // null
  ===
  @use "sass:map"
  $font-weights: ("regular": 400, "medium": 500, "bold": 700)

  @debug map.get($font-weights, "medium")  // 500
  @debug map.get($font-weights, "extra-bold")  // null
{% endcodeExample %}
